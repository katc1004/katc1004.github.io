{
  "name": "katc1004.github.io",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "gh-pages-clean": "rm -rf node_modules/gh-pages/.cache",
    "deploy": "gh-pages-clean gatsby build && cp ./CNAME public/ && gh-pages -d public -b master",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.22.15",
    "gatsby-plugin-sass": "^2.3.4",
    "gatsby-source-filesystem": "^2.3.11",
    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "gh-pages": "^3.0.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katc1004/katc1004.github.io"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
